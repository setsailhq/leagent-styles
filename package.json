{
  "name": "leagent-style",
  "version": "1.0.0",
  "description": "Leagent styling Lib",
  "homepage": "https://github.com/setsailhq/leagent-styles",
  "repository": "setsailhq/leagent-styles",
  "license": "MIT",
  "author": {
    "name": "Leagent",
    "email": "team@leagent.com",
    "url": "https://leagent.com"
  },
  "browserslist": [
    "> 0.2%",
    "not dead",
    "not ie > 0"
  ],
  "files": [
    "src/assets",
    "src/helpers",
    "src/hooks",
    "src/lib",
    "src/styles"
  ],
  "keywords": [
    "leagent",
    "styles"
  ],
  "sideEffects": false,
  "main": "src/styles.scss",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url src/styles.scss styles.scss",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch",
    "preinstall": "npm i smoothscroll-polyfill && npm i body-scroll-lock",
    "postinstall": "cd ../../ && npm i ./node_modules/leagent-style/src/lib/fslightbox-react-pro-1.4.2.tgz --no-save"
  },
  "dependencies": {
    "react": "^16.13.1",
    "smoothscroll-polyfill": "^0.4.4",
    "body-scroll-lock": "^3.1.5"
  },
  "devDependencies": {
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "node-sass": "^4.14.1"
  },
  "bugs": {
    "url": "https://github.com/setsailhq/leagent-styles/issues"
  }
}
