{
  "name": "leagent-style",
  "version": "1.0.0",
  "description": "Leagent styling Lib",
  "homepage": "https://github.com/setsailhq/leagent-styles",
  "repository": "setsailhq/leagent-styles",
  "license": "MIT",
  "author": {
    "name": "Leagent",
    "email": "team@leagent.com",
    "url": "https://leagent.com"
  },
  "browserslist": [
    "> 0.2%",
    "not dead",
    "not ie > 0"
  ],
  "files": [
    "src/styles"
  ],
  "keywords": [
    "leagent",
    "styles"
  ],
  "sideEffects": false,
  "main": "src/styles.scss",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url src/styles.scss styles.scss",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch"
  },
  "dependencies": {},
  "devDependencies": {
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "node-sass": "^4.14.1"
  }
}
